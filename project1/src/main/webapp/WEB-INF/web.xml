<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.shyam.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>com.shyam.servlets.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StudentList</servlet-name>
  	<display-name>StudentList</display-name>
  	<description></description>
  	<servlet-class>com.shyam.servlets.model.StudentList</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ActionServlet</servlet-name>
  	<display-name>ActionServlet</display-name>
  	<description></description>
  	<servlet-class>com.shyam.servlets.ActionServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StudentFormServlet</servlet-name>
  	<display-name>StudentFormServlet</display-name>
  	<description></description>
  	<servlet-class>com.shyam.form.StudentFormServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/logoutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StudentList</servlet-name>
  	<url-pattern>/studentList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ActionServlet</servlet-name>
  	<url-pattern>/actionServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StudentFormServlet</servlet-name>
  	<url-pattern>/studentFormServlet</url-pattern>
  </servlet-mapping>
</web-app>
